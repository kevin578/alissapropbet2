- url = "/contest/#{@contest.id}"
.container.pt-8{'data-controller' => 'edit-contest', 'data-edit-contest-url-value'=> url}
  = form_with model: @article, url: url, method: :patch do |f|
    %div
    = render 'contest/edit_components/name_input', f: f
    = render 'contest/edit_components/status_input', f: f
    .mt-6
      = render 'contest/edit_components/new_prop_button'
      .mt-10{'data-edit-contest-target'=> 'container'}
        = render 'contest/edit_components/prop', f: f, hidden: true
        - @contest.props.each do |p|
          = render 'contest/edit_components/prop', f: f, prop: p
      .mt-12
        = render 'contest/edit_components/save_button'



